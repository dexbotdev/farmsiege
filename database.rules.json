{
   "rules": {
      ".read": false,
      ".write": false,
      "highscores": {
         ".read": true,
         ".write": true,
         ".indexOn": [
            "score",
            ".value",
            "name"
         ],
         ".validate": "newData.hasChildren(['name', 'score']) && newData.child('name').isString() &&newData.child('score').isNumber() && newData.child('score').val() > 0"
      }
   }
}
